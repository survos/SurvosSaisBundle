{
    "name": "survos/sais-bundle",
    "description": "Integration with Survos Async Image Server",
    "keywords": [
        "symfony-ux",
        "thumbnails",
        "resize",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3||^8.0",
        "symfony/console": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/http-kernel": "^7.3||^8.0",
        "twig/twig": "^3.4",
        "symfony/http-client": "^7.3||^8.0",
        "zircote/swagger-php": "^5.1.4",
        "symfony/validator": "^7.3||^8.0",
        "symfony/webhook": "^7.3 | ^8.0",
        "fguillot/json-rpc": "^1.3",
        "symfony/serializer": "^7.3||^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3||^8.0",
        "symfony/framework-bundle": "^7.3||^8.0",
        "symfony/phpunit-bridge": "^7.3||^8.0",
        "symfony/twig-bundle": "^7.3||^8.0",
        "symfony/var-dumper": "^7.3||^8.0",
        "symfony/messenger": "^7.3||^8.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\SaisBundle\\": "src/"
        }
    }
}
